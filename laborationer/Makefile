.PHONY: clean All

All:
	@echo "----------Building project:[ 1-210315 - Debug ]----------"
	@cd "1-210315" && "$(MAKE)" -f  "1-210315.mk" && "$(MAKE)" -f  "1-210315.mk" PostBuild
clean:
	@echo "----------Cleaning project:[ 1-210315 - Debug ]----------"
	@cd "1-210315" && "$(MAKE)" -f  "1-210315.mk" clean
